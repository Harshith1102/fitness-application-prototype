    "lg": "0.75rem",
    "xl": "1rem",
    "xxl": "1.5rem",
    "full": "9999px"
  },
  "typography": {
    "fontFamily": {
      "primary": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
      "mono": "'JetBrains Mono', 'Fira Code', Consolas, monospace"
    },
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "xxl": "1.5rem",
      "xxxl": "2rem",
      "display": "3rem"
    },
    "fontWeight": {
      "light": "300",
      "normal": "400",
      "medium": "500",
      "semibold": "600",
      "bold": "700"
    }
  },
  "animations": {
    "fast": "150ms",
    "normal": "250ms",
    "slow": "350ms",
    "slower": "500ms"
  }
